        <packageUrl regex="true">^pkg:maven/ch\.qos\.logback/logback\-core@.*$</packageUrl>
        <cve>CVE-2023-6378</cve>
        <cve>CVE-2023-6481</cve>
    </suppress>
    <suppress>
        <packageUrl regex="true">^pkg:maven/ch\.qos\.logback/logback\-classic@.*$</packageUrl>
        <cve>CVE-2023-6378</cve>
        <cve>CVE-2023-6481</cve>
    </suppress>
