        <packageUrl regex="true">^pkg:maven/io\.netty/netty\-.*@.*$</packageUrl>
        <cve>CVE-2023-44487</cve>
    </suppress>

    <!-- https://issues.apache.org/jira/browse/CASSANDRA-17966 -->
