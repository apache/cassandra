          cat /tmp/split_dtest_tests_j17_dtests_offheap_final.txt

          source ~/env3.7/bin/activate
          export PATH=$JAVA_HOME/bin:$PATH
          if [ -n 'CQLSH_PYTHON=/usr/bin/python3.7' ]; then
