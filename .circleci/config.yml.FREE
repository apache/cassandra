        path: /tmp/results
    - store_artifacts:
        path: /tmp/dtest
        destination: dtest_j17_dtests_offheap
    - store_artifacts:
        path: ~/cassandra-dtest/logs
        destination: dtest_j17_dtests_offheap_logs
